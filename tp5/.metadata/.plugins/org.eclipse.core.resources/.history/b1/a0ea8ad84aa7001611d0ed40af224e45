package tp5;

import java.util.ArrayList;
import java.util.List;

public class Ex2 {
	public class Q2{
		public int data = 5;
		
	}
	
	public class Q2Son extends Q2{
		public Q2Son(){super.data = 4;}
	}
	
	public static int getData(List<? extends Q2> q){
		return q.get(0).data;
	}
	
	public static void main(String[] args) {
		Ex2 e = new Ex2();
		List<Q2Son> preuve = new ArrayList<Q2Son>();
		for (int i = 0; i < 10; i++) {
			Q2Son preuve = e.new Q2Son();
		}
		
		
		
		
	}

}